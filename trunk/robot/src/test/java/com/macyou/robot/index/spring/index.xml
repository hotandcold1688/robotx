<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd"
	default-autowire="byName">

	<bean class="com.macyou.robot.common.SpringBeanGetter" />
	<bean id="simpleRobotConfig" class="com.macyou.robot.config.RobotConfig"></bean>
	<bean id="simpleRobot" class="com.macyou.robot.SimpleRobot">
	   <property name="config" ref="simpleRobotConfig" />
	</bean>

	<bean id="robotManager" class="com.macyou.robot.RobotManager" init-method="init" />
	<bean id="indexManager" class="com.macyou.robot.IndexManager">
	</bean>

</beans>